{
  "name": "boilerplate-koa-redux-react",
  "version": "1.0.0",
  "description": "A boilerplate for a Koa Redux React application with Webpack, Mocha and SASS",
  "main": "index.js",
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-3",
      "es2015-node5"
    ],
    "plugins": [
      "transform-runtime",
      "transform-object-rest-spread"
    ]
  },
  "scripts": {
    "test": "`npm bin`/mocha --compilers js:babel-core/register --require ./app/tests/test_helper.js '*/tests/**/*.@(js|jsx)'",
    "test:watch": "npm run test -- --watch",
    "webserver": "nodemon --exec npm run babel-node api/src/server.js",
    "koa": "nodemon -L --exec npm run babel-node api/src/server.js",
    "build": "`npm bin`/webpack",
    "dev": "`npm bin`/webpack-dev-server",
    "lint": "`npm bin`/eslint . --ext .js --ext .jsx",
    "deploy": "NODE_ENV=production `npm bin`/webpack -p",
    "start": "npm run dev",
    "clean": "rm -rf app/dist app/build",
    "babel-node": "babel-node --ignore='node_modules'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mezod/boilerplate-koa-redux-react.git"
  },
  "keywords": [
    "boilerplate",
    "koa",
    "redux",
    "react",
    "webpack",
    "mocha",
    "sass"
  ],
  "author": "Joan Boixad√≥s",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mezod/boilerplate-koa-redux-react/issues"
  },
  "homepage": "https://github.com/mezod/boilerplate-koa-redux-react#readme",
  "dependencies": {
    "axios": "^0.9.1",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.3.15",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.5.0",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-node5": "^1.1.2",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-preset-stage-3": "^6.5.0",
    "bookshelf": "^0.9.1",
    "css-loader": "^0.23.0",
    "file-loader": "^0.8.5",
    "history": "^2.0.0",
    "html-webpack-plugin": "^2.7.1",
    "immutable": "^3.7.5",
    "isomorphic-fetch": "^2.2.0",
    "knex": "^0.10.0",
    "koa": "^2.0.0-alpha.3",
    "koa-convert": "^1.2.0",
    "koa-cors": "0.0.16",
    "koa-route": "^2.4.2",
    "koa-router": "^7.0.1",
    "mysql": "^2.9.0",
    "node-sass": "^3.4.2",
    "nodemon": "^1.9.0",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.0",
    "react-router": "^2.0.0",
    "redux": "^3.0.4",
    "redux-logger": "^2.6.0",
    "redux-promise": "^0.5.1",
    "redux-thunk": "^1.0.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0-beta4",
    "chai": "^3.4.1",
    "chai-immutable": "^1.5.3",
    "eslint": "^2.2.0",
    "eslint-config-airbnb": "^6.0.2",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^4.1.0",
    "jsdom": "^8.0.4",
    "mocha": "^2.3.4",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^3.0.1",
    "webpack-dev-server": "^1.14.0"
  }
}
